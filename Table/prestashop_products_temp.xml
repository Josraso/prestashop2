<?xml version="1.0" encoding="UTF-8"?>
<!--
    Tabla temporal para almacenar productos descargados de PrestaShop
-->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>session_id</name>
        <type>character varying(128)</type>
        <null>NO</null>
    </column>
    <column>
        <name>products_data</name>
        <type>text</type>
        <null>YES</null>
    </column>
    <column>
        <name>fecha_descarga</name>
        <type>timestamp</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>prestashop_products_temp_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
</table>
